'						F.Intrinsic.UI.ChangeCallerProperty(V.Passed.000064,"LOCK",0)
'					F.Intrinsic.Control.EndIf
				F.Intrinsic.Control.EndIf
			F.Intrinsic.Control.EndIf
			F.ODBC.conx!rstApp.Close
		F.Intrinsic.Control.EndIf
		F.ODBC.conx!rst.Close
	'F.Intrinsic.Control.EndIf
F.Intrinsic.Control.Else
